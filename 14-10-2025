

Question
class Solution {
    String removeDuplicates(String s) {
        int n = s.length();
        HashSet<Character> set = new HashSet<>();
        StringBuilder str = new StringBuilder();
        for(char ch : s.toCharArray()){
            if(!set.contains(ch)){
                str.append(ch);
                set.add(ch);
            }
        }
        return str.toString();
    }
}
